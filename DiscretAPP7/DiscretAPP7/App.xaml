<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DiscretAPP7.App">
	<Application.Resources>
        <ResourceDictionary>
            <OnPlatform x:TypeArguments="x:String" x:Key="BoldFont">
                <On Platform="Android" Value="agency_fb_bold.ttf#AgencyFB_Bold" />
                <On Platform="iOS" Value="agency_fb_bold" />
                <On Platform="WinPhone" Value="agency_fb_bold" />
            </OnPlatform>
            <OnPlatform x:TypeArguments="x:String" x:Key="NormalFont">
                <On Platform="Android" Value="agency_fb.ttf#AgencyFB" />
                <On Platform="iOS" Value="agency_fb" />
                <On Platform="WinPhone" Value="agency_fb" />
            </OnPlatform>
        </ResourceDictionary>
    </Application.Resources>
</Application>